# MedFocus — Environment Variables
# Copy to .env and fill in values

# Database (Cloud SQL MySQL via Unix socket for Cloud Run)
DATABASE_URL=mysql://medfocus:MedFocus@2026!Gcp@localhost/medfocus?socket=/cloudsql/viralgram-4-0-rodrigo:southamerica-east1:viralgram-mysql

# JWT Authentication
JWT_SECRET=your-secure-jwt-secret-here

# Google Gemini API
GEMINI_API_KEY=your-gemini-api-key

# GCP Project
GOOGLE_CLOUD_PROJECT=viralgram-4-0-rodrigo

# Stripe (optional)
STRIPE_SECRET_KEY=
STRIPE_WEBHOOK_SECRET=
VITE_STRIPE_PUBLISHABLE_KEY=

# Google Cloud Storage
GCS_BUCKET=medfocus-uploads
USE_GCS=true

# App
VITE_APP_ID=medfocus
APP_URL=https://medfocus-app-969630653332.southamerica-east1.run.app
CORS_ORIGIN=*
OWNER_EMAIL=rodrigo.goncalves@uisa.com.br

# ─── Medical API Integrations ───────────────────────────

# PubMed / NCBI (opcional - aumenta limites de requisicao)
NCBI_API_KEY=

# Infermedica API (obrigatorio para triagem de sintomas)
# Obtenha em: https://developer.infermedica.com/
INFERMEDICA_APP_ID=
INFERMEDICA_APP_KEY=

# Google Cloud Healthcare API (FHIR, HL7v2, DICOM)
# Usa o mesmo projeto GCP definido em GOOGLE_CLOUD_PROJECT
GCP_HEALTHCARE_LOCATION=southamerica-east1
GCP_HEALTHCARE_DATASET=medfocus-health
GCP_FHIR_STORE=medfocus-fhir
GCP_DICOM_STORE=medfocus-dicom
GCP_HL7V2_STORE=medfocus-hl7v2

# Metriport API (consolidacao de dados e wearables)
# Obtenha em: https://metriport.com/
METRIPORT_API_KEY=
METRIPORT_FACILITY_ID=
METRIPORT_API_URL=https://api.metriport.com
METRIPORT_SANDBOX=true
